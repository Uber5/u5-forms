{
  "name": "u5-forms",
  "version": "0.0.3",
  "description": "Efficiently add forms to a react/redux/material-ui app",
  "main": "dist/index.js",
  "scripts": {
    "build": "babel src --out-dir dist --source-maps",
    "build:watch": "npm run build -- --watch",
    "prepublish": "npm run clean && npm run build",
    "test": "jest",
    "test:watch": "npm test -- --watch",
    "clean": "rimraf dist"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/Uber5/u5-forms.git"
  },
  "keywords": [
    "react",
    "redux",
    "uber5",
    "forms",
    "crud"
  ],
  "author": "Chris Oloff <chris@uber5.com> (http://about.uber5.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Uber5/u5-forms/issues"
  },
  "homepage": "https://github.com/Uber5/u5-forms#readme",
  "devDependencies": {
    "babel-cli": "6.22.2",
    "babel-jest": "18.0.0",
    "babel-polyfill": "6.22.0",
    "babel-preset-es2015": "6.22.0",
    "babel-preset-react": "6.22.0",
    "babel-preset-stage-2": "6.22.0",
    "enzyme": "2.7.1",
    "jest": "18.1.0",
    "react-addons-test-utils": "15.4.2",
    "react-test-renderer": "15.4.2",
    "rimraf": "2.5.4",
    "source-map-support": "0.4.10"
  },
  "peerDependencies": {
    "react": "^15.0.0 || ^16.0.0",
    "redux": "^2.0.0 || ^3.0.0",
    "material-ui": "^0.16",
    "react-tap-event-plugin": "^1.0.0 || ^2.0.0",
    "react-redux": "^4.0.0 || ^5.0.0"
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-2"
    ]
  },
  "jest": {
    "rootDir": "src",
    "setupTestFrameworkScriptFile": "../test/setup.js"
  }
}
